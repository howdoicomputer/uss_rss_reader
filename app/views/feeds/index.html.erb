<h1>All Feeds</h1>

<ul id="feeds">
  <% @feeds.each do |feed| %>
    <li>
      <%= link_to feed.title, feed_path(feed) %>
      <%= "(" %>
      <%= link_to "x", feed_path(feed), method: :delete %>
      <%= ")" %>
    </li>
  <% end %>
</ul>

<%= link_to "Create a New Feed", new_feed_path, class: "new_feed" %>
